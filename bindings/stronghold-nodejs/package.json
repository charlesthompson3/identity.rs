{
  "name": "stronghold-nodejs",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "stronghold-nodejs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.4.2",
    "@types/node": "^17.0.19",
    "cross-var": "^1.1.0",
    "ts-node": "^10.5.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "npm run create-npm-dir && npm run build:napi && npm run build:ts",
    "build:ts": "tsc node_adapter/stronghold_storage.ts",
    "build:napi": "cross-var \"napi build --platform --release $npm_config_napi_args\"",
    "build:napi:debug": "napi build --platform",
    "example:stronghold": "ts-node ./examples/src/node.ts",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "create-npm-dir": "napi create-npm-dir --target ./ ",
    "test": "npm run example:stronghold create_did"
  },
  "peerDependencies": {
    "@iota/identity-wasm": "^0.5.0-dev.4"
  }
}
